<?xml version="1.0"?>
<config>
		<port-list>
        <ethernet>
            <name>vEth0</name>
            <state>up</state>
            <mode>trunk</mode>
            <native>1</native>
        </ethernet>
        <ethernet>
            <name>vEth1</name>
            <state>up</state>
            <mode>trunk</mode>
            <native>1</native>
        </ethernet>
    </port-list>
    <bridge-list>
    	<bridge>
    		<name>br1</name>
    		<interface>eif0</interface>
	    	<vlan>1</vlan>
	    	<port>vEth0</port>
	    	<port>vEth1</port>
    	</bridge>
    	<bridge>
    		<name>br2</name>
    		<interface>eif1</interface>
	    	<vlan>2</vlan>
	    	<port>vEth0</port>
	    	<port>vEth1</port>
    	</bridge>
    </bridge-list>
    <interface-list>
        <interface>
            <name>eif0</name>
            <address>192.168.101.1/24</address>
        </interface>
        <interface>
            <name>eif1</name>
            <address>192.168.102.1/24</address>
        </interface>
    </interface-list>
    <ip-forward>
        <interface>eif0</interface>
        <interface>eif1</interface>
    </ip-forward>
</config>
